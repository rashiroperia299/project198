        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Marks</servlet-name>
        <url-pattern>/Marks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>attendance</servlet-name>
        <url-pattern>/attendance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>courseenrolled</servlet-name>
        <url-pattern>/courseenrolled</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>parentInfo</servlet-name>
        <url-pattern>/parentInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>personalInfo</servlet-name>
        <url-pattern>/personalInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sign</servlet-name>
        <url-pattern>/sign</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>timetable</servlet-name>
        <url-pattern>/timetable</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
